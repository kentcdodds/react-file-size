{
  "name": "react-file-size",
  "version": "1.0.0",
  "description": "Show file size changes over versions of React",
  "author": "Elijah Manor",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "react-file-size": "./index.js"
  },
  "config": {
    "port": "1337"
  },
  "scripts": {
    "//": "Gather all the script info from cdnjs",
    "start": "#🚀 Start the server \n\t node index.js",
    "poststart": "npm run build && npm run server",
    "pretest": "npm run lint",
    "//": "Run the mocha unit tests",
    "test": "#⚡️ Run the Unit Tests \n\t BABEL_ENV=test mocha spec/ --require babel-register",
    "cover": "nyc npm t",
    "postcover": "rm -rf .nyc_output",
    "cover:open": "opn coverage/lcov-report/index.html",
    "//": "Lint all the things",
    "lint": "#🚦 Lint JavaScript and CSS \n\t npm-run-all lint:**",
    "lint:js": "eslint --cache --fix ./",
    "lint:css": "stylelint \"**/*.scss\" --syntax scss",
    "lint:css:fix": "stylefmt -R src/",
    "watch": "npm-run-all --parallel watch:*",
    "watch:test": "npm t -- --watch",
    "watch:lint": "onchange 'src/**/*.js' 'src/**/*.scss' -- npm run lint",
    "build": "npm-run-all build:*",
    "prebuild": "cross-var rm -rf public/$npm_package_version",
    "build:html": "cross-var jade --obj data.json src/index.jade --out public/$npm_package_version/",
    "build:css": "cross-var \"node-sass src/index.scss | postcss -c .postcssrc.json | cssmin > public/$npm_package_version/index.min.css\"",
    "build:js": "cross-var \"mustache data.json src/index.mustache.js | uglifyjs > public/%npm_package_version%/index.min.js\"",
    "server": "npm-run-all --parallel server:*",
    "server:create": "cross-var http-server public/$npm_package_version -p $npm_package_config_port",
    "server:launch": "cross-var opn http://localhost:$npm_package_config_port",
    "precommit": "npm run lint"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "spec/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "gzip-size": "^3.0.0",
    "gzip-size-cli": "^1.0.0",
    "inquirer": "^1.2.2",
    "lodash": "^4.0.1",
    "log-update": "^1.0.2",
    "normalize.scss": "^0.1.0",
    "ora": "^0.3.0",
    "pretty-bytes": "^4.0.2",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "shelljs": "^0.7.5",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^16.0.0",
    "babel-plugin-espower": "^2.3.1",
    "chai": "^3.5.0",
    "cross-var": "^1.0.0",
    "cssmin": "^0.4.3",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.9.1",
    "eslint-config-leankit": "^3.0.0",
    "gzipped": "0.0.5",
    "http-server": "^0.9.0",
    "husky": "^0.11.9",
    "jade": "^1.11.0",
    "jscs": "^3.0.7",
    "mocha": "^3.1.0",
    "mustache": "^2.2.1",
    "node-sass": "^3.11.2",
    "npm-run-all": "^3.1.1",
    "nyc": "^8.3.2",
    "onchange": "^3.0.2",
    "opn-cli": "^3.1.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.6.0",
    "power-assert": "^1.4.1",
    "proxyquire": "^1.7.10",
    "push-dir": "^0.2.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "stylefmt": "^4.3.1",
    "stylelint": "^7.5.0",
    "uglify-js": "^2.6.2"
  }
}
